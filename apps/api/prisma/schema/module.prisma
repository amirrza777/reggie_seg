model Module {
  id           Int          @id @default(autoincrement())
  name         String
  moduleLeads  ModuleLead[] @ignore
  userModules  UserModule[] @ignore
  teams        Team[]       @ignore
  peerAssessments PeerAssessment[] @ignore
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt
}
