model UserModule {
  enterpriseId String
  userId       Int
  moduleId     Int
  module       Module @relation(fields: [moduleId, enterpriseId], references: [id, enterpriseId])
  user         User   @relation(fields: [userId, enterpriseId], references: [id, enterpriseId])

  @@id([enterpriseId, userId, moduleId])
  @@index([moduleId])
  @@index([userId])
}
